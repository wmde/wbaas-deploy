restorePodRunning: false

job:
  cronSchedule: "0 0 * * *"

storage:
  scratchDiskSpace: 8Gi
  gcs:
    bucketName: wikibase-dev-sql-backup
    serviceAccountSecretName: api-serviceaccount
    uploadToBucket: true

resources:
  job:
    requests:
      cpu: 10m
      memory: 50Mi
    limits:
      cpu: 750m
      memory: 500Mi
  restorePod:
    requests:
      cpu: 10m
      memory: 50Mi
    limits:
      cpu: 750m
      memory: 500Mi