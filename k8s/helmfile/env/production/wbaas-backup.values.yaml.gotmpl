image:
  repository: ghcr.io/wmde/wbaas-backup
  tag: v0.3.1
  pullPolicy: Always

#changed to test if the SQL logical backups still work after T349817, change back later
job:
  cronSchedule: "0 0 28 * *"

scratchDiskSpace: 128Gi

storage:
  bucketName: wikibase-cloud-sql-backup
  serviceAccountSecretName: api-serviceaccount
  uploadToBucket: true

db:
  load:
    verbosity: 1
  dump:
    verbosity: 1

restorePodRunning: false
