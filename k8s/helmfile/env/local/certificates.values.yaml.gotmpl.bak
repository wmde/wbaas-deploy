certificates:
  - name: wikibase-local-tls
    commonName: wbaas-local-test.de
    dnsNames:
      - '*.wbaas-local-test.de'
      - 'wbaas-local-test.de'
    secretName: wikibase-local-tls
    issuerRef: wikibase-local-issuer
  - name: argocd-server-tls
    namespace: argocd
    commonName: argo.wbaas-local-test.de
    dnsNames:
      - 'argo.wbaas-local-test.de'
    secretName: argocd-server-tls
    issuerRef: wikibase-local-issuer
